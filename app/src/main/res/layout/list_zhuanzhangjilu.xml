<?xml version="1.0" encoding="utf-8"?>


    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            >
            

<LinearLayout 
              android:orientation="horizontal"
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:weightSum="6.6"
              >

        <TextView
                style="?attr/main_shoufeimingxi_listtv_style"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/tv1"
                android:layout_weight="0.1"
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                />

        <TextView
                style="?attr/main_shoufeimingxi_listtv_style"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/tv2"
                android:layout_weight="1"
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                />

        <TextView
                style="?attr/main_shoufeimingxi_listtv_style"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/tv3"
                android:gravity="center"
                android:layout_weight="1.7"
                android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                />

        <TextView
                style="?attr/main_shoufeimingxi_listtv_style"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/tv4"
                android:gravity="center"
                android:layout_weight="1.5"
                android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                />

        <TextView
                style="?attr/main_shoufeimingxi_listtv_style"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/tv5"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                />
        
         <TextView
                style="?attr/main_shoufeimingxi_listtv_style"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:id="@+id/tv6"
                android:gravity="center"
                android:layout_weight="1.3"
                android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="7dp"
                />
    
</LinearLayout>

   <Button
            style="@style/buttonFunctionDetailStyle1Port"
            android:background="?attr/button_function_noborder_background_port"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:id="@+id/btn"
            android:gravity="center"
            android:padding="0dp"
            android:text="@string/zhuanzhang_btn_findback"
            />
</LinearLayout>